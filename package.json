{
  "name": "angular-sandbox",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:mobile": "ng serve --open --host 192.168.0.215 --disable-host-check",
    "build": "ng build",
    "lazyUi": "npm i && ng build lazy-ui && cd dist/lazy-ui && npm publish --access public",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:electron": "ng build --base-href ./ && electron .",
    "winpackage": "electron-packager . AngularSandbox --platform=win32 --arch=x64 --overwrite --app-version=1.0.0 --build-version=1.0.0 --win32metadata.ProductName=AngularSandbox --FileDescription=AngularSandbox --appname=angularsandbox --win32metadata.CompanyName=Judell --app-copyright=Judell --icon=favicon.ico",
    "archiveapp": "asar pack AngularSandbox-win32-x64/resources/app/ AngularSandbox-win32-x64/resources/app.asar",
    "packit": "ng build --base-href ./ && npm run winpackage && npm run archiveapp"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.7",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.7",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@jaytech/lazy-ui": "^1.3.0",
    "asar": "^3.1.0",
    "bootstrap": "^5.1.3",
    "rxjs": "~6.6.0",
    "semantic-ui-css": "^2.4.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular/cli": "~12.2.7",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "electron": "^16.0.5",
    "electron-packager": "^15.4.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.1.1",
    "typescript": "~4.3.5"
  }
}
